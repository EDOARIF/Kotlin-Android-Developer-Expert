            app:layout_constraintStart_toStartOf="@+id/txt_lbl_str_event"
            app:layout_constraintTop_toBottomOf="@+id/txt_str_time_event" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txt_unlocked_event"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_lbl_score_part"
        tools:text="unlocked" />

        <View
            android:layout_width="match_parent"
            android:layout_height="4dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:background="@color/colorPrimary"
            app:layout_constraintEnd_toEndOf="@+id/txt_str_events"
            app:layout_constraintStart_toStartOf="@+id/txt_str_events"
            app:layout_constraintTop_toBottomOf="@+id/txt_str_events" />

        <View
